@page
@using DevNewsBot.App.ViewModel
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4" id="target">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <button class="btn btn-primary btn-lg px-4"
            hx-get=""
            hx-target="#target">
        Click Me
    </button>
    @await Html.PartialAsync("Shared/_Articles", new ArticlesViewModel() { Cursor = Model.Cursor, Articles = Model.Articles, Page = Model.ArticlesPage, PageSize = Model.PageSize })
</div>